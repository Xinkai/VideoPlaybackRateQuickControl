{
  "manifest_version": 3,

  "name": "Youtube Playback Rate Quick Control",
  "description": "Easily control video playback speed",
  "version": "1.9.2",

  "content_scripts": [
    {
      "matches"   : ["<all_urls>"],
      "js"        : ["content.js"],
      "run_at"    : "document_start",
      "all_frames": true
    }
  ],

  "host_permissions": [
    "*://*.youtube.com/*",
    "*://*.bilibili.com/*",
    "*://*.odysee.com/*",
    "*://*.reddit.com/*"
  ]
}
